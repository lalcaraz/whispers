{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "ignoreDeprecations": "6.0",
    "baseUrl": "./",
    "paths": {
      "@hooks/*": ["src/hooks/*"],
      "@components/*": ["src/components/*"],
      "@views/*": ["src/views/*"],
      "@locales/*": ["src/locales/*"],
      "@navigation": ["src/navigation/index.tsx"],
      "@/*": ["src/*"]
    }
  }
}
